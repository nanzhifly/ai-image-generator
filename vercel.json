{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/server.js" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "env": {
    "DEEPSEEK_API_KEY": "@deepseek_api_key"
  }
} 